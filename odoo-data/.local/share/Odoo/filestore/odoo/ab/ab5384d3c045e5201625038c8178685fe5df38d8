            run: "click",
        },
        {
            content: "Hover on attachment",
            trigger: '.o-mail-Message .o-mail-AttachmentCard:contains("file2.txt")',
            run: "hover",
        },
        {
            content: "Click on expand button",
            trigger: '.o-mail-Message [title="Expand"]',
            run: "click",
